{
  "manifest_version": 3,
  "name": "Linker",
  "version": "1.0.0",
  "description": "Generates a QR code for the current tab's URL.",
  "permissions": ["tabs"],
  "action": {
    "default_popup": "src/popup.html",
    "default_icon": {
      "16": "Linker-no-bg.png",
      "32": "Linker.png",
      "48": "Linker.png",
      "128": "Linker.png"
    }
  },
  "key": "",
  "browser_specific_settings": {
    "gecko": {
      "id": "adasimobenio@gmail.com",
      "strict_min_version": "109.0"
    }
  }
}
